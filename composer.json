{
    "name": "wieni/wmusersnap",
    "type": "drupal-module",
    "description": "Integrate the Usersnap feedback widget with your Drupal website.",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "drupal/core": "^8.5 || ^9.0"
    },
    "require-dev": {
        "composer-runtime-api": "^2.0",
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
